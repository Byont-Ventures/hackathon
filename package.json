{
  "name": "byont.io",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "fe:dev": "next dev",
    "fe:build": "next build",
    "fe:start": "next start",
    "fe:lint": "next lint",
    "fe:test": "DOTENV_CONFIG_PATH=.env.test node src/bin/mockthereum.js -c 'jest --setupFiles=dotenv/config'",
    "fe:test:watch": "DOTENV_CONFIG_PATH=.env.test node src/bin/mockthereum.js -c 'jest --watch --setupFiles=dotenv/config'",
    "sc:build": "forge build --optimize",
    "sc:test": "forge test -vvv",
    "sc:test:watch": "forge test -vvv --watch",
    "sc:coverage": "forge coverage -vvv",
    "sc:lint": "solhint ./src/smart-contracts/*.sol",
    "lint": "yarn fe:lint && yarn sc:lint",
    "format": "prettier --loglevel warn --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore .",
    "postbuild": "next-sitemap"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "dependencies": {
    "@byont/mocketh": "^1.3.2",
    "@next/font": "^13.0.0",
    "@rainbow-me/rainbowkit": "^0.8.0",
    "classnames": "^2.3.2",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "next": "^13.0.0",
    "next-seo": "^5.11.1",
    "next-sitemap": "^3.1.29",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "wagmi": "^0.7.7"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@next/bundle-analyzer": "^12.3.1",
    "@openzeppelin/contracts": "^4.7.3",
    "@openzeppelin/contracts-upgradeable": "^4.7.3",
    "@tailwindcss/typography": "^0.5.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.11.7",
    "@types/react": "^18.0.24",
    "@types/react-dom": "18.0.8",
    "@types/three": "^0.144.0",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.0.0",
    "eslint": "^8.26.0",
    "eslint-config-next": "^13.0.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "jest-text-transformer": "^1.0.4",
    "jsdom-testing-mocks": "^1.6.0",
    "mockthereum": "^0.1.0",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "prettier-plugin-jsdoc": "^0.4.2",
    "prettier-plugin-solidity": "^1.0.0-beta.24",
    "sol-merger": "^4.1.1",
    "solhint": "^3.3.7",
    "solhint-plugin-prettier": "^0.0.5",
    "tailwindcss": "^3.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
